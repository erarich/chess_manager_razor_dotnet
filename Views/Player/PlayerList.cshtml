@model List<Player>

<h2>Players List</h2>

<table class="table">
    <thead>
        <tr>
            <th>Player Name</th>
            <th>Score</th>
            <!-- Adicione mais colunas conforme necessário -->
        </tr>
    </thead>
    <tbody>
        @foreach (var player in Model)
        {
            <tr>
                <td>@player.Name</td>
                <!-- Adicione mais colunas conforme necessário -->
            </tr>
        }
    </tbody>
</table>
